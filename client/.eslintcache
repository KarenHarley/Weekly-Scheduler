e.log(e.target.id);\n      setDay(e.target.id);\n    }\n  };\n  useEffect(() => {\n    if (Auth.loggedIn()) {\n      setId(Auth.getProfile().data._id);\n    }\n  }, []);\n  return (\n    <div className=\"tasks-wrapper\">\n      <h3>Tasks Page</h3>\n\n      <div className=\"select-day-div\">\n        <ul onClick={selectDay}>\n          {days.map((loopingDay, i) => {\n            return (\n              <li\n                type=\"li\"\n                id={loopingDay}\n                key={loopingDay}\n                className={day === loopingDay ? \"selected\" : \"nav-link\"}\n              >\n                {loopingDay}\n              </li>\n            );\n          })}\n        </ul>\n      </div>\n      {Auth.loggedIn() ? (\n        <div className=\"all-tasks-wrapper\">\n          {loading ? (\n            <div>Loading...</div>\n          ) : (\n            <>\n              {tasks.length >= 1 ? (\n                <div className=\"all-tasks\">\n                  {tasks.map((task, i) => {\n                    return <Task data={task} key={i} />;\n                  })}\n                 </div>\n              ) : (\n                <p className=\"no-tasks\">No Tasks</p>\n               \n              )}\n            </>\n          )}\n          <div className=\"create-form-div\">\n            <CreateForm setDay={setDay} day={day} />\n          </div>\n        </div>\n      ) : (\n        <p>\n          You need to be logged in to see your Tasks. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default Tasks;\n","/Users/karenvillagomez/Weekly-Shedular/client/src/components/Task.js",[],"/Users/karenvillagomez/Weekly-Shedular/client/src/utils/queries.js",[],"/Users/karenvillagomez/Weekly-Shedular/client/src/pages/OneTask.js",[],"/Users/karenvillagomez/Weekly-Shedular/client/src/pages/EditTask.js",["73"],"import { Link } from \"react-router-dom\";\nimport { useQuer